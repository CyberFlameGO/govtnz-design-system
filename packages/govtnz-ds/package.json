{
  "name": "@govtnz/ds",
  "version": "0.1.2",
  "description": "Government Design System",
  "author": "New Zealand Government",
  "homepage": "https://github.com/GOVTNZ/govtnz-design-system/",
  "scripts": {
    "build:release:debugger": "babel-node --inspect --expose-gc --max_old_space_size=8192 --extensions '.ts' ./src/index.ts",
    "build:release": "yarn build:source-release && yarn build:compile",
    "build:source-release": "babel-node --max_old_space_size=8192 --expose-gc --extensions '.ts' ./src/index.ts",
    "build:compile": "rimraf ./build && yarn babel build_src --out-dir build && yarn tsc && cp build_src/css build/css -r && yarn tsc && cp build_src/scss build/scss -r && cp build_src/html build/html -r && cp build_src/mustache build/mustache -r && cp build_src/silverstripe-components build/silverstripe-components -r  && cp build_src/static build/static -r && cp build_src/twig-embed build/twig-embed -r && cp build_src/vue-js build/vue-js -r && cp build_src/vue-ts build/vue-ts -r",
    "test": "jest",
    "test:typescript": "tsc --project ./ --noEmit",
    "rimraf": "exit 0"
  },
  "license": "MIT",
  "devDependencies": {
    "@angular/core": "^8.2.14",
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@govtnz/ds-common": "0.0.1",
    "@govtnz/ds-upstream": "0.0.1",
    "@springload/metatemplate": "^0.5.0",
    "@types/node": "^10.12.19",
    "@vue/cli-service": "^3.8.4",
    "argparse": "^1.0.10",
    "compilation-templates": "0.0.1",
    "es6-promise-pool": "^2.5.0",
    "filesize": "^4.1.2",
    "format-duration": "^1.3.1",
    "glob": "^7.1.3",
    "glob-promise": "^3.4.0",
    "jest": "^24.8.0",
    "jsdom": "^14.0.0",
    "lodash": "^4.17.11",
    "mkdirp": "^1.0.4",
    "normalize-selector": "^0.2.0",
    "postcss": "^7.0.14",
    "postcss-merge-rules": "^4.0.2",
    "postcss-safe-parser": "^4.0.1",
    "progress": "^2.0.3",
    "rimraf": "^2.6.3",
    "rmfr": "^2.0.0",
    "styled-components": "^4.2.0",
    "svg-to-dataurl": "^1.0.0",
    "typescript": "^3.2.4"
  }
}
