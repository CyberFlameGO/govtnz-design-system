{
  "name": "@govtnz/ds",
  "version": "0.2.2",
  "description": "Government Design System",
  "author": "New Zealand Government",
  "homepage": "https://github.com/GOVTNZ/govtnz-design-system/",
  "scripts": {
    "build:release:debugger": "babel-node --inspect --expose-gc --max_old_space_size=8192 --extensions '.ts' ./src/index.ts",
    "build:release": "yarn build:source-release && yarn build:compile",
    "build:source-release": "babel-node --max_old_space_size=8192 --expose-gc --extensions '.ts' ./src/index.ts",
    "build:compile": "yarn babel build_src --out-dir build && yarn tsc",
    "test:typescript": "tsc --project ./ --noEmit",
    "node-modules-clear": "rimraf ./node_modules",
    "cache-clear": "rimraf ./yarn-error.log"
  },
  "license": "MIT",
  "devDependencies": {
    "@angular/core": "^9.1.11",
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@springload/metatemplate": "1.0.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@vue/cli-service": "^4.4.3",
    "argparse": "^1.0.10",
    "filesize": "^6.1.0",
    "format-duration": "^1.3.1",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "glob-promise": "^3.4.0",
    "jest": "^26.0.1",
    "jsdom": "^16.2.2",
    "lodash": "^4.17.15",
    "mkdirp": "^1.0.4",
    "normalize-selector": "^0.2.0",
    "postcss": "^7.0.32",
    "postcss-merge-rules": "^4.0.3",
    "postcss-safe-parser": "^4.0.2",
    "progress": "^2.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1",
    "typescript": "^3.9.5"
  }
}
