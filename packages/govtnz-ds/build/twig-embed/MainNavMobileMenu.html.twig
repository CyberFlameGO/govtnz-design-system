<button
aria-controls="{{ navId | escape('html_attr') }}"
aria-expanded="{{ isOpen | escape('html_attr') }}"
class="g-main-nav-mobile-menu{% if isOpen == "true" %}g-main-nav-mobile-menu--open{% elseif isOpen == "false" %}g-main-nav-mobile-menu--closed{% endif %}"
name="{{ name | escape('html_attr') }}"
type="{{ type | escape('html_attr') }}"
>
  
  
{% block children %} Menu (example text) {% endblock %}
  
  
<svg
class="g-main-nav-mobile-menu__icon"
viewBox="0 0 16 16"
xmlns="http://www.w3.org/2000/svg"
>
  
    
<path
d="M7.992 9.55l5.6-5.6a1 1 0 0 1 1.415 0l.688.689a1 1 0 0 1 0 1.414L8.7 13.049a1 1 0 0 1-1.414 0L.29 6.053a1 1 0 0 1 0-1.414l.689-.689a1 1 0 0 1 1.414 0l5.6 5.6z"
fill="currentColor"
fill-rule="nonzero"
/>
  
  
</svg>
  

</button>