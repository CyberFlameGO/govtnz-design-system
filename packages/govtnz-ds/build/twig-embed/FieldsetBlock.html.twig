<div
class="g-fieldsetBlock-form-group"
>
  
        
<fieldset
aria-describedby="{{ hintId | escape('html_attr') }}"
class="g-fieldset"
>
  
          
<legend
class="g-fieldset__legend"
>
{% block legend %}Legend text{% endblock %}
</legend>
  
          
  
            
<div
class="g-fieldsetBlock-hint"
id="{{ hintId | escape('html_attr') }}"
>
{% block hint %}Hint text{% endblock %}
</div>
  
          
  
          
<div
>
{% block children %}Fieldset contents{% endblock %}
</div>
  
        
</fieldset>
  
      
</div>