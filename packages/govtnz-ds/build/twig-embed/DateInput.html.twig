<div
class="g-dateInput-form-group{% if errorId %} g-dateInput-form-group--error{% endif %}"
>
  
  
<fieldset
aria-describedby="{{ hintId | escape('html_attr') }} {{ errorId | escape('html_attr') }}"
class="g-dateInput-fieldset"
role="group"
>
  
    
<legend
class="g-dateInput-fieldset__legend"
>
  
      
{% block label %}Example label{% endblock %}
  
    
</legend>
  
    
<span
class="g-dateInput-hint"
id="{{ hintId | escape('html_attr') }}"
>
  
      
{% block hint %}Example hint{% endblock %}
  
    
</span>
  
    
  
      
<span
class="g-dateInput-error-message"
id="{{ errorId | escape('html_attr') }}"
>
  
        
<span
class="g-dateInput-visually-hidden"
>
  Error: 
</span>
  
        
{% block error %}Example error{% endblock %}
  
      
</span>
  
    
  
    
<div
class="g-dateInput"
id="{{ id | escape('html_attr') }}"
>
  
      
<div
class="g-dateInput__item"
>
  
        
<div
class="g-dateInput-form-group"
>
  
          
<label
class="g-dateInput-label g-dateInput__label"
for="{{ dayId | escape('html_attr') }}"
>
  
            Day
          
</label>
  
          
<input
class="g-dateInput-input g-dateInput__input g-dateInput-input--width-2{% if error %} g-dateInput-input--error{% endif %}"
id="{{ dayId | escape('html_attr') }}"
maxlength="2"
name="{{ dayName | escape('html_attr') }}"
pattern="[0-9]*"
type="text"
value="{{ value | escape('html_attr') }}"
required="{% if required %}required{% endif %}"
disabled="{% if disabled %}disabled{% endif %}"
readonly="{% if readOnly %}readOnly{% endif %}"
autofocus="{% if autoFocus %}autoFocus{% endif %}"
spellcheck="{% if spellCheck %}spellCheck{% endif %}"
autocomplete="{{ autoComplete | escape('html_attr') }}"
/>
  
        
</div>
  
      
</div>
  
      
<div
class="g-dateInput__item"
>
  
        
<div
class="g-dateInput-form-group"
>
  
          
<label
class="g-dateInput-label g-dateInput__label"
for="{{ monthId | escape('html_attr') }}"
>
  
            Month
          
</label>
  
          
<input
class="g-dateInput-input g-dateInput__input g-dateInput-input--width-2{% if error %} g-dateInput-input--error{% endif %}"
id="{{ monthId | escape('html_attr') }}"
maxlength="2"
name="{{ yearName | escape('html_attr') }}"
pattern="[0-9]*"
type="text"
value="{{ value2 | escape('html_attr') }}"
required="{% if required %}required{% endif %}"
disabled="{% if disabled %}disabled{% endif %}"
readonly="{% if readOnly %}readOnly{% endif %}"
autofocus="{% if autoFocus %}autoFocus{% endif %}"
spellcheck="{% if spellCheck %}spellCheck{% endif %}"
autocomplete="{{ autoComplete | escape('html_attr') }}"
/>
  
        
</div>
  
      
</div>
  
      
<div
class="g-dateInput__item"
>
  
        
<div
class="g-dateInput-form-group"
>
  
          
<label
class="g-dateInput-label g-dateInput__label"
for="{{ yearId | escape('html_attr') }}"
>
  
            Year
          
</label>
  
          
<input
class="g-dateInput-input g-dateInput__input g-dateInput-input--width-4{% if error %} g-dateInput-input--error{% endif %}"
id="{{ yearId | escape('html_attr') }}"
maxlength="4"
name="{{ yearName | escape('html_attr') }}"
pattern="[0-9]*"
type="text"
value="{{ value3 | escape('html_attr') }}"
required="{% if required %}required{% endif %}"
disabled="{% if disabled %}disabled{% endif %}"
readonly="{% if readOnly %}readOnly{% endif %}"
autofocus="{% if autoFocus %}autoFocus{% endif %}"
spellcheck="{% if spellCheck %}spellCheck{% endif %}"
autocomplete="{{ autoComplete | escape('html_attr') }}"
/>
  
        
</div>
  
      
</div>
  
    
</div>
  
  
</fieldset>
  

</div>