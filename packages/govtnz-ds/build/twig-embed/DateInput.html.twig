<div
class="g-dateInput-form-group"
>
  
  
<fieldset
aria-describedby="{{dobHint}}"
class="g-dateInput-fieldset"
role="group"
>
  
    
<legend
class="g-dateInput-fieldset__legend"
>
  What is your date of birth?
</legend>
  
    
<span
class="g-dateInput-hint"
id="{{dobHint}}"
>
  
      For example, 31 3 1980
    
</span>
  
    
<div
class="g-date-input"
id="{{dob}}"
>
  
      
<div
class="g-date-input__item"
>
  
        
<div
class="g-dateInput-form-group"
>
  
          
<label
class="g-dateInput-label g-date-input__label"
for="{{dobDay}}"
>
  
            Day
          
</label>
  
          
<input
class="g-dateInput-input g-date-input__input g-dateInput-input--width-2"
id="{{dobDay}}"
name="{{name}}"
pattern="[0-9]*"
type=""
disabled="{{#disabled}}disabled{{/disabled}}"
readonly="{{#readOnly}}readOnly{{/readOnly}}"
autofocus="{{#autoFocus}}autoFocus{{/autoFocus}}"
value="{{value}}"
spellcheck="{{#spellCheck}}spellCheck{{/spellCheck}}"
autocomplete=""
/>
  
        
</div>
  
      
</div>
  
      
<div
class="g-date-input__item"
>
  
        
<div
class="g-dateInput-form-group"
>
  
          
<label
class="g-dateInput-label g-date-input__label"
for="{{dobMonth}}"
>
  
            Month
          
</label>
  
          
<input
class="g-dateInput-input g-date-input__input g-dateInput-input--width-2"
id="{{dobMonth}}"
name="{{name2}}"
pattern="[0-9]*"
type=""
disabled="{{#disabled2}}disabled2{{/disabled2}}"
readonly="{{#readOnly2}}readOnly2{{/readOnly2}}"
autofocus="{{#autoFocus2}}autoFocus2{{/autoFocus2}}"
value="{{value2}}"
spellcheck="{{#spellCheck2}}spellCheck2{{/spellCheck2}}"
autocomplete=""
/>
  
        
</div>
  
      
</div>
  
      
<div
class="g-date-input__item"
>
  
        
<div
class="g-dateInput-form-group"
>
  
          
<label
class="g-dateInput-label g-date-input__label"
for="{{dobYear}}"
>
  
            Year
          
</label>
  
          
<input
class="g-dateInput-input g-date-input__input g-dateInput-input--width-4"
id="{{dobYear}}"
name="{{name3}}"
pattern="[0-9]*"
type=""
disabled="{{#disabled3}}disabled3{{/disabled3}}"
readonly="{{#readOnly3}}readOnly3{{/readOnly3}}"
autofocus="{{#autoFocus3}}autoFocus3{{/autoFocus3}}"
value="{{value3}}"
spellcheck="{{#spellCheck3}}spellCheck3{{/spellCheck3}}"
autocomplete=""
/>
  
        
</div>
  
      
</div>
  
    
</div>
  
  
</fieldset>
  

</div>