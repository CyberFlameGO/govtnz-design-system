<a
class="g-backLinkWithCustomText-back-link"
href="{{ href | escape('html_attr') }}"
rel="{{ rel | escape('html_attr') }}"
target="{% if target == "Blank" %}_blank{% elseif target == "Top" %}_top{% elseif target == "Self" %}_self{% elseif target == "Parent" %}_parent{% endif %}"
>
{% block children %}Example text{% endblock %}
</a>