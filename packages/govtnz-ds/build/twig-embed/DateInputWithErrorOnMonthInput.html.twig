<div
class="g-dateInputWithErrorOnMonthInput-form-group g-dateInputWithErrorOnMonthInput-form-group--error"
>
  
  
<fieldset
aria-describedby="{{dobMonthErrorHint}} {{dobMonthErrorError}}"
class="g-dateInputWithErrorOnMonthInput-fieldset"
role="group"
>
  
    
<legend
class="g-dateInputWithErrorOnMonthInput-fieldset__legend"
>
  What is your date of birth?
</legend>
  
    
<span
class="g-dateInputWithErrorOnMonthInput-hint"
id="{{dobMonthErrorHint}}"
>
  
      For example, 31 3 1980
    
</span>
  
    
<span
class="g-dateInputWithErrorOnMonthInput-error-message"
id="{{dobMonthErrorError}}"
>
  
      Error message goes here
    
</span>
  
    
<div
class="g-dateInputWithErrorOnMonthInput-date-input"
id="{{dobMonthError}}"
>
  
      
<div
class="g-dateInputWithErrorOnMonthInput-date-input__item"
>
  
        
<div
class="g-dateInputWithErrorOnMonthInput-form-group"
>
  
          
<label
class="g-dateInputWithErrorOnMonthInput-label g-dateInputWithErrorOnMonthInput-date-input__label"
for="{{dobMonthErrorDay}}"
>
  
            Day
          
</label>
  
          
<input
class="g-dateInputWithErrorOnMonthInput-input g-dateInputWithErrorOnMonthInput-date-input__input g-dateInputWithErrorOnMonthInput-input--width-2"
id="{{dobMonthErrorDay}}"
name="{{name}}"
pattern="[0-9]*"
type=""
disabled="{{#disabled}}disabled{{/disabled}}"
readonly="{{#readOnly}}readOnly{{/readOnly}}"
autofocus="{{#autoFocus}}autoFocus{{/autoFocus}}"
value="{{value}}"
spellcheck="{{#spellCheck}}spellCheck{{/spellCheck}}"
autocomplete=""
/>
  
        
</div>
  
      
</div>
  
      
<div
class="g-dateInputWithErrorOnMonthInput-date-input__item"
>
  
        
<div
class="g-dateInputWithErrorOnMonthInput-form-group"
>
  
          
<label
class="g-dateInputWithErrorOnMonthInput-label g-dateInputWithErrorOnMonthInput-date-input__label"
for="{{dobMonthErrorMonth}}"
>
  
            Month
          
</label>
  
          
<input
class="g-dateInputWithErrorOnMonthInput-input g-dateInputWithErrorOnMonthInput-date-input__input g-dateInputWithErrorOnMonthInput-input--width-2 g-dateInputWithErrorOnMonthInput-input--error"
id="{{dobMonthErrorMonth}}"
name="{{name2}}"
pattern="[0-9]*"
type=""
disabled="{{#disabled2}}disabled2{{/disabled2}}"
readonly="{{#readOnly2}}readOnly2{{/readOnly2}}"
autofocus="{{#autoFocus2}}autoFocus2{{/autoFocus2}}"
value="{{value2}}"
spellcheck="{{#spellCheck2}}spellCheck2{{/spellCheck2}}"
autocomplete=""
/>
  
        
</div>
  
      
</div>
  
      
<div
class="g-dateInputWithErrorOnMonthInput-date-input__item"
>
  
        
<div
class="g-dateInputWithErrorOnMonthInput-form-group"
>
  
          
<label
class="g-dateInputWithErrorOnMonthInput-label g-dateInputWithErrorOnMonthInput-date-input__label"
for="{{dobMonthErrorYear}}"
>
  
            Year
          
</label>
  
          
<input
class="g-dateInputWithErrorOnMonthInput-input g-dateInputWithErrorOnMonthInput-date-input__input g-dateInputWithErrorOnMonthInput-input--width-4"
id="{{dobMonthErrorYear}}"
name="{{name3}}"
pattern="[0-9]*"
type=""
disabled="{{#disabled3}}disabled3{{/disabled3}}"
readonly="{{#readOnly3}}readOnly3{{/readOnly3}}"
autofocus="{{#autoFocus3}}autoFocus3{{/autoFocus3}}"
value="{{value3}}"
spellcheck="{{#spellCheck3}}spellCheck3{{/spellCheck3}}"
autocomplete=""
/>
  
        
</div>
  
      
</div>
  
    
</div>
  
  
</fieldset>
  

</div>