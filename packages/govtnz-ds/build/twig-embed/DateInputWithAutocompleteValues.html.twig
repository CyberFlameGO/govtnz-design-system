<div
class="g-dateInputWithAutocompleteValues-form-group"
>
  
  
<fieldset
aria-describedby="{{dobWithAutocompleteAttributeHint}}"
class="g-dateInputWithAutocompleteValues-fieldset"
role="group"
>
  
    
<legend
class="g-dateInputWithAutocompleteValues-fieldset__legend"
>
  What is your date of birth?
</legend>
  
    
<span
class="g-dateInputWithAutocompleteValues-hint"
id="{{dobWithAutocompleteAttributeHint}}"
>
  
      For example, 31 3 1980
    
</span>
  
    
<div
class="g-dateInputWithAutocompleteValues-date-input"
id="{{dobWithAutocompleteAttribute}}"
>
  
      
<div
class="g-dateInputWithAutocompleteValues-date-input__item"
>
  
        
<div
class="g-dateInputWithAutocompleteValues-form-group"
>
  
          
<label
class="g-dateInputWithAutocompleteValues-label g-dateInputWithAutocompleteValues-date-input__label"
for="{{dobWithAutocompleteAttributeDay}}"
>
  
            Day
          
</label>
  
          
<input
autocomplete=""
class="g-dateInputWithAutocompleteValues-input g-dateInputWithAutocompleteValues-date-input__input g-dateInputWithAutocompleteValues-input--width-2"
id="{{dobWithAutocompleteAttributeDay}}"
name="{{name}}"
pattern="[0-9]*"
type=""
disabled="{{#disabled}}disabled{{/disabled}}"
readonly="{{#readOnly}}readOnly{{/readOnly}}"
autofocus="{{#autoFocus}}autoFocus{{/autoFocus}}"
value="{{value}}"
spellcheck="{{#spellCheck}}spellCheck{{/spellCheck}}"
/>
  
        
</div>
  
      
</div>
  
      
<div
class="g-dateInputWithAutocompleteValues-date-input__item"
>
  
        
<div
class="g-dateInputWithAutocompleteValues-form-group"
>
  
          
<label
class="g-dateInputWithAutocompleteValues-label g-dateInputWithAutocompleteValues-date-input__label"
for="{{dobWithAutocompleteAttributeMonth}}"
>
  
            Month
          
</label>
  
          
<input
autocomplete=""
class="g-dateInputWithAutocompleteValues-input g-dateInputWithAutocompleteValues-date-input__input g-dateInputWithAutocompleteValues-input--width-2"
id="{{dobWithAutocompleteAttributeMonth}}"
name="{{name2}}"
pattern="[0-9]*"
type=""
disabled="{{#disabled2}}disabled2{{/disabled2}}"
readonly="{{#readOnly2}}readOnly2{{/readOnly2}}"
autofocus="{{#autoFocus2}}autoFocus2{{/autoFocus2}}"
value="{{value2}}"
spellcheck="{{#spellCheck2}}spellCheck2{{/spellCheck2}}"
/>
  
        
</div>
  
      
</div>
  
      
<div
class="g-dateInputWithAutocompleteValues-date-input__item"
>
  
        
<div
class="g-dateInputWithAutocompleteValues-form-group"
>
  
          
<label
class="g-dateInputWithAutocompleteValues-label g-dateInputWithAutocompleteValues-date-input__label"
for="{{dobWithAutocompleteAttributeYear}}"
>
  
            Year
          
</label>
  
          
<input
autocomplete=""
class="g-dateInputWithAutocompleteValues-input g-dateInputWithAutocompleteValues-date-input__input g-dateInputWithAutocompleteValues-input--width-2"
id="{{dobWithAutocompleteAttributeYear}}"
name="{{name3}}"
pattern="[0-9]*"
type=""
disabled="{{#disabled3}}disabled3{{/disabled3}}"
readonly="{{#readOnly3}}readOnly3{{/readOnly3}}"
autofocus="{{#autoFocus3}}autoFocus3{{/autoFocus3}}"
value="{{value3}}"
spellcheck="{{#spellCheck3}}spellCheck3{{/spellCheck3}}"
/>
  
        
</div>
  
      
</div>
  
    
</div>
  
  
</fieldset>
  

</div>