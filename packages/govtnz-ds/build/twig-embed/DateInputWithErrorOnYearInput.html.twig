<div
class="g-dateInputWithErrorOnYearInput-form-group g-dateInputWithErrorOnYearInput-form-group--error"
>
  
  
<fieldset
aria-describedby="{{dobYearErrorHint}} {{dobYearErrorError}}"
class="g-dateInputWithErrorOnYearInput-fieldset"
role="group"
>
  
    
<legend
class="g-dateInputWithErrorOnYearInput-fieldset__legend"
>
  What is your date of birth?
</legend>
  
    
<span
class="g-dateInputWithErrorOnYearInput-hint"
id="{{dobYearErrorHint}}"
>
  
      For example, 31 3 1980
    
</span>
  
    
<span
class="g-dateInputWithErrorOnYearInput-error-message"
id="{{dobYearErrorError}}"
>
  
      Error message goes here
    
</span>
  
    
<div
class="g-dateInputWithErrorOnYearInput-date-input"
id="{{dobYearError}}"
>
  
      
<div
class="g-dateInputWithErrorOnYearInput-date-input__item"
>
  
        
<div
class="g-dateInputWithErrorOnYearInput-form-group"
>
  
          
<label
class="g-dateInputWithErrorOnYearInput-label g-dateInputWithErrorOnYearInput-date-input__label"
for="{{dobYearErrorDay}}"
>
  
            Day
          
</label>
  
          
<input
class="g-dateInputWithErrorOnYearInput-input g-dateInputWithErrorOnYearInput-date-input__input g-dateInputWithErrorOnYearInput-input--width-2"
id="{{dobYearErrorDay}}"
name="{{name}}"
pattern="[0-9]*"
type=""
disabled="{{#disabled}}disabled{{/disabled}}"
readonly="{{#readOnly}}readOnly{{/readOnly}}"
autofocus="{{#autoFocus}}autoFocus{{/autoFocus}}"
value="{{value}}"
spellcheck="{{#spellCheck}}spellCheck{{/spellCheck}}"
autocomplete=""
/>
  
        
</div>
  
      
</div>
  
      
<div
class="g-dateInputWithErrorOnYearInput-date-input__item"
>
  
        
<div
class="g-dateInputWithErrorOnYearInput-form-group"
>
  
          
<label
class="g-dateInputWithErrorOnYearInput-label g-dateInputWithErrorOnYearInput-date-input__label"
for="{{dobYearErrorMonth}}"
>
  
            Month
          
</label>
  
          
<input
class="g-dateInputWithErrorOnYearInput-input g-dateInputWithErrorOnYearInput-date-input__input g-dateInputWithErrorOnYearInput-input--width-2"
id="{{dobYearErrorMonth}}"
name="{{name2}}"
pattern="[0-9]*"
type=""
disabled="{{#disabled2}}disabled2{{/disabled2}}"
readonly="{{#readOnly2}}readOnly2{{/readOnly2}}"
autofocus="{{#autoFocus2}}autoFocus2{{/autoFocus2}}"
value="{{value2}}"
spellcheck="{{#spellCheck2}}spellCheck2{{/spellCheck2}}"
autocomplete=""
/>
  
        
</div>
  
      
</div>
  
      
<div
class="g-dateInputWithErrorOnYearInput-date-input__item"
>
  
        
<div
class="g-dateInputWithErrorOnYearInput-form-group"
>
  
          
<label
class="g-dateInputWithErrorOnYearInput-label g-dateInputWithErrorOnYearInput-date-input__label"
for="{{dobYearErrorYear}}"
>
  
            Year
          
</label>
  
          
<input
class="g-dateInputWithErrorOnYearInput-input g-dateInputWithErrorOnYearInput-date-input__input g-dateInputWithErrorOnYearInput-input--width-4 g-dateInputWithErrorOnYearInput-input--error"
id="{{dobYearErrorYear}}"
name="{{name3}}"
pattern="[0-9]*"
type=""
disabled="{{#disabled3}}disabled3{{/disabled3}}"
readonly="{{#readOnly3}}readOnly3{{/readOnly3}}"
autofocus="{{#autoFocus3}}autoFocus3{{/autoFocus3}}"
value="{{value3}}"
spellcheck="{{#spellCheck3}}spellCheck3{{/spellCheck3}}"
autocomplete=""
/>
  
        
</div>
  
      
</div>
  
    
</div>
  
  
</fieldset>
  

</div>