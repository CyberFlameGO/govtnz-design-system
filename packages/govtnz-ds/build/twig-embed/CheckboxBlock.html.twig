<div
class="g-checkboxes__item"
>
  
  
<input
aria-describedby="{{ hintId | escape('html_attr') }}"
class="g-checkboxes__input"
id="{{ checkboxId | escape('html_attr') }}"
type="checkbox"
disabled="{% if disabled %}disabled{% endif %}"
readonly="{% if readOnly %}readOnly{% endif %}"
autofocus="{% if autoFocus %}autoFocus{% endif %}"
name="{{ name | escape('html_attr') }}"
value="{{ value | escape('html_attr') }}"
checked="{% if checked %}checked{% endif %}"
/>
  
  
<label
class="g-checkboxBlock-label g-checkboxes__label"
for="{{ checkboxId | escape('html_attr') }}"
>
{% block label %}Label text{% endblock %}
</label>
  
  
  
    
<div
class="g-checkboxBlock-hint g-checkboxes__hint"
id="{{ hintId | escape('html_attr') }}"
>
  
      
{% block hint %}Hint text{% endblock %}
  
    
</div>
  
  
  

</div>