<div
class="g-fileUpload-form-group"
>
  
  
<label
class="g-fileUpload-label"
for="{{ fileUpload1 | escape('html_attr') }}"
>
  
    Upload a file
  
</label>
  
  
<input
class="g-file-upload"
id="{{ fileUpload1 | escape('html_attr') }}"
name="{{ name | escape('html_attr') }}"
type="file"
disabled="{{ if disabled }}disabled{{ endif }}"
readonly="{{ if readOnly }}readOnly{{ endif }}"
autofocus="{{ if autoFocus }}autoFocus{{ endif }}"
/>
  

</div>