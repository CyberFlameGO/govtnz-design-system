<div
class="g-breadcrumbs"
>
  
  
<ol
class="g-breadcrumbs__list"
>
  
    
<li
class="g-breadcrumbs__list-item"
>
  
      
<a
class="g-breadcrumbs__link"
href="{{ href | escape('html_attr') }}"
rel="{{ rel | escape('html_attr') }}"
target="{% if target == "Blank" %}_blank{% elseif target == "Top" %}_top{% elseif target == "Self" %}_self{% elseif target == "Parent" %}_parent{% endif %}"
>
  Section
</a>
  
    
</li>
  
    
<li
class="g-breadcrumbs__list-item"
>
  
      
<a
class="g-breadcrumbs__link"
href="{{ href2 | escape('html_attr') }}"
rel="{{ rel2 | escape('html_attr') }}"
target="{% if target2 == "Blank" %}_blank{% elseif target2 == "Top" %}_top{% elseif target2 == "Self" %}_self{% elseif target2 == "Parent" %}_parent{% endif %}"
>
  Sub-section
</a>
  
    
</li>
  
  
</ol>
  

</div>