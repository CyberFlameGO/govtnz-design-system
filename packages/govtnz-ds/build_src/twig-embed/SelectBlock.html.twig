<div
class="{% if errorId %}g-form-group--error{% endif %}"
>
  
  
<label
class="g-selectBlock-label"
for="{{ selectId | escape('html_attr') }}"
>
  
    
{% block label %}Example label text{% endblock %}
  
  
</label>
  
  
<select
class="g-selectBlock-select{% if error %} g-selectBlock-select--error{% endif %}"
id="{{ selectId | escape('html_attr') }}"
name="{{ name | escape('html_attr') }}"
multiple="{% if multiple %}multiple{% endif %}"
>
  Options
</select>
  

</div>